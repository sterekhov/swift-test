<?xml version='1.0' encoding='utf-8'?>
<plugin id="plugin-swift-test" version="1.0.0" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
    <name>SwiftTest</name>
    <js-module name="SwiftTest" src="www/SwiftTest.js">
        <clobbers target="SwiftTest" />
    </js-module>
    <platform name="ios">
        <config-file target="config.xml" parent="/*">
            <feature name="SwiftTest">
                <param name="ios-package" value="SwiftTest" />
            </feature>
        </config-file>
        <source-file src="src/ios/SwiftTest.swift" />
    </platform>
</plugin>
